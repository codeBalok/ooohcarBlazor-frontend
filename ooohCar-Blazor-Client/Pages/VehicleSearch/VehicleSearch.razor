                <!-- Advanced search start -->
                <div class="widget advanced-search2">
                    <h3 class="sidebar-title">Search Cars</h3>
                    <div class="s-border"></div>
                    <div class="m-border"></div>
                    <form method="GET">
                        <div class="form-group">
                            <div id="options-content">
                                <h3 class="sidebar-title">Make</h3>
                                @if (lstMake.Count > 0)
                                {
                                    @foreach (var make in lstMake)
                                    {

                                        if (make != null)
                                        {

                                            <div class="s-border"></div>
                                            <div class="m-border"></div>
                                            <div class="checkbox checkbox-theme checkbox-circle">
                                                <input id="checkbox_@make.Id" type="checkbox" data-target="#Model_@make.Id" aria-expanded="false" data-toggle="collapse">
                                                <label for="checkbox_@make.Id">
                                                    @make.Name
                                                </label>
                                                @if (@make.Model != null)
                                                {
                                                    <div class="sidebar-title collapse sub-item" id="Model_@make.Id">
                                                        <h5 class="collapse-item">Model</h5>
                                                        @foreach (var model in @make.Model)
                                                        {
                                                            if (model != null)
                                                            {
                                                                <div class="s-border"></div>
                                                                <div class="m-border"></div>
                                                                <div class="checkbox checkbox-theme checkbox-circle">
                                                                    <input id="checkboxMode_@model.Id" type="checkbox" data-target="#ModelVarient_@model.Id" aria-expanded="false" data-toggle="collapse">
                                                                    <label for="checkboxMode_@model.Id">
                                                                        @model.Name
                                                                    </label>
                                                                    @if (@model.Varient != null)
                                                                    {
                                                                        <div class="sidebar-title collapse sub-item" id="ModelVarient_@model.Id">
                                                                            <h5 class="collapse-item">Varient</h5>
                                                                            @foreach (var varient in @model.Varient)
                                                                            {
                                                                                if (varient != null)
                                                                                {
                                                                                    <div class="s-border"></div>
                                                                                    <div class="m-border"></div>
                                                                                    <div class="checkbox checkbox-theme checkbox-circle">
                                                                                        <input id="checkboxVarient_@varient.Id" type="checkbox">
                                                                                        <label for="checkboxVarient_@varient.Id">
                                                                                            @varient.Name
                                                                                        </label>
                                                                                    </div>
                                                                                }
                                                                            }
                                                                        </div>
                                                                    }
                                                                </div>

                                                            }
                                                        }
                                                    </div>
                                                }
                                            </div>
                                        }
                                    }
                                }
                                <br>
                            </div>
                        </div>

                        <div class="range-slider clearfix">
                            <label @onclick="@(e => checktest())">Price</label>
                            <div data-min="0" data-max="150000" data-min-name="min_price" data-max-name="max_price" data-unit="USD" class="range-slider-ui ui-slider ui-slider-horizontal ui-widget ui-widget-content ui-corner-all" aria-disabled="false"><span class="min-value">0 USD</span> <span class="max-value">150000 USD</span><input class="current-min" type="hidden" name="min_price" value="0"><input class="current-max" type="hidden" name="max_price" value="150000"><div class="ui-slider-range ui-widget-header ui-corner-all" style="left: 0%; width: 100%;"></div><a class="ui-slider-handle ui-state-default ui-corner-all" href="#" style="left: 0%;"></a><a class="ui-slider-handle ui-state-default ui-corner-all" href="#" style="left: 100%;"></a></div>
